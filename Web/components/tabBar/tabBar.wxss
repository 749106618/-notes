/* tabBar */
.tabBar{
    width: 100%;
    height: 96rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #FFF;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 99;
    border-top:2px solid #EFEFEF;
}

/* tabBar-item */
.tabBar-item{
    text-align: center;
}

.tabBar-item3{
    width: 33.33%;
}

.tabBar-item4{
    width: 25%;
}

.tabBar-img{
    margin: 10rpx auto 10rpx;
    display: block;
}

.tabBar-img1{
    width: 40rpx;
    height: 40rpx;
}

.tabBar-img2{
    width: 47rpx;
    height: 40rpx;
}

.tabBar-img3{
    width: 56rpx;
    height: 44rpx;
}

.tabBar-img4{
    width: 40rpx;
    height: 40rpx;
}

.tabBar-text{
    font-size: 20rpx;
    color: #A1A1A1;
    letter-spacing: -0.33px;
}

.tabBar-text-active{
    color: #4B89F3;
}