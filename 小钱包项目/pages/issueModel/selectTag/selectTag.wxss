.select{
    background-color: #fff;
    margin-top: 18rpx;
}
.select_item{
    height: 100rpx;
    display: flex;
    align-content: center;
    line-height: 100rpx;
    position: relative;
}

.select_item image{
    width: 49rpx;
    height: 49rpx;
    right: 24rpx;
    top: 26rpx;
    position: absolute;
    z-index: 999;
}

.select_item .classify{
    font-size: 30rpx;
    color: #222222;
    margin-left: 25rpx;
}
.select_item .line{
    width: 725rpx;
    height: 2rpx;
    background-color: #EEEEEE;
    position: absolute;
    left: 25rpx;
    bottom: 0;
}
.issue {
    width:701rpx;
    height:91rpx;
    border-radius:55rpx;
    background-color:#56D6CC;
    color:#ffffff;
    font-size:37rpx;
    text-align:center;
    line-height:91rpx;
    margin:0 auto;
    margin-top:59rpx;
}
